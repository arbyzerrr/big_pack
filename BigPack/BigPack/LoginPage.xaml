<Page x:Class="BigPack.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BigPack"
      mc:Ignorable="d" 
      d:DesignHeight="202.447"
      Title="LoginPage" Width="397">

    <Grid>
        <Button x:Name="BtnEnter" Content="Войти" HorizontalAlignment="Center" VerticalAlignment="Top" Click="ButtonEnter_Click" Margin="0,136,0,0"/>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Login" VerticalAlignment="Top" Margin="114,19,0,0"/>
        <TextBox x:Name="textBox_login" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Top" Width="172" Margin="0,40,0,0"/>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Margin="114,71,0,0" />
        <PasswordBox x:Name="passwordBox_password" HorizontalAlignment="Center" VerticalAlignment="Top" Width="172" Margin="0,92,0,0"/>
    </Grid>
</Page>
